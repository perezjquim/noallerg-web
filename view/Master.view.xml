<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	controllerName="noallerg.controller.Master"
	xmlns:semantic="sap.m.semantic">
	<semantic:MasterPage title="{i18n>TITLE_MARKERS}">
		<semantic:content>
			<List
				items="{markers>/}"
				growing="true"
				growingScrollToLoad="true">
				<items>
					<ObjectListItem
						type="Navigation"
						title="{markers>title}"
						press="onSelectionChange">
					</ObjectListItem>
				</items>
			</List>
		</semantic:content>
	</semantic:MasterPage>
</mvc:View>
